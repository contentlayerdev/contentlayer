{
  "name": "@contentlayer/source-files",
  "version": "0.2.6",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./schema/defs": {
      "import": "./dist/schema/defs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "schema": [
        "./dist/schema"
      ]
    }
  },
  "files": [
    "./dist/*.{js,ts,map}",
    "./dist/!(__test__)/**/*.{js,ts,map}",
