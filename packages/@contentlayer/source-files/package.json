  "scripts": {
    "test": "TZ='GMT' FORCE_COLOR=1 vitest"
  },
  "peerDependencies": {
    "date-fns": "2.x"
  },
  "peerDependenciesMeta": {
    "date-fns": {
      "optional": true
    }
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "chokidar": "^3.5.3",
    "date-fns": "^2",
    "date-fns-tz": "^1.3.4",
    "fast-glob": "^3.2.11",
    "gray-matter": "^4.0.3",
    "micromatch": "^4.0.5",
    "ts-pattern": "^4.0.5",
    "unified": "^10.1.2",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^17.0.45",
    "@types/yaml": "^1.9.7",
    "faker": "^5.5.3",
    "vite": "^3.0.4",
    "vitest": "^0.20.2"
  }
}
