{
  "name": "contentlayer-stackbit-yaml-generator",
  "version": "0.2.6-dev.7",
  "type": "module",
  "bin": "./bin/cli.cjs",
  "exports": {
    ".": "./dist/lib/index.js",
    "./types": "./dist/types/index.js"
  },
  "types": "./dist/lib/index.d.ts",
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types"
      ]
    }
  },
  "files": [
    "./dist/*.{js,ts,map}",
    "./dist/!(__test__)/**/*.{js,ts,map}",
    "./src",
    "./package.json"
  ],
  "scripts": {
    "test": "FORCE_COLOR=1 vitest"
  },
  "dependencies": {
    "@contentlayer/core": "workspace:*",
    "@contentlayer/utils": "workspace:*",
    "@stackbit/sdk": "^0.2.32",
    "clipanion": "^3.2.0-rc.11",
    "typanion": "3.8.0",
    "yaml": "^2.1.0"
  },
  "devDependencies": {
    "contentlayer": "workspace:*",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.12.4"
  }
}
