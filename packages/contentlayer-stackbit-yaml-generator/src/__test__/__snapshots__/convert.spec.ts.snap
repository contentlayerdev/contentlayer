"stackbitVersion: ~0.3.0
nodeVersion: \\">=12\\"
models:
  Blog:
    label: Blog
    fields:
      - name: title
        required: true
        description: The title of the page
        label: Title
        type: string
      - name: seo
        required: false
        type: model
        models:
          - SEO
      - name: body
        required: true
        description: Markdown file body
        type: markdown
    file: blog.md
    type: page
    singleInstance: true
  Landing:
    label: Landing Page
    fields:
      - name: title
        required: true
        description: The title of the page
        label: Title
        type: string
      - name: sections
        required: false
        description: Page sections
        label: Sections
        type: list
        items:
          type: model
          models:
            - SectionContent
            - SectionCta
            - SectionFaq
            - SectionFeatures
            - SectionHero
            - SectionPosts
            - SectionPricing
            - SectionReviews
            - SectionContact
      - name: seo
        required: false
        type: model
        models:
          - SEO
      - name: body
        required: true
        description: Markdown file body
        type: markdown
    match:
      - contact.md
      - features.md
      - index.md
      - pricing.md
    type: page
    singleInstance: false
  Page:
    label: Page
    fields:
      - name: title
        required: true
        description: The title of the page
        type: string
      - name: subtitle
        required: false
        description: The text shown below the page title
        type: string
      - name: image
        required: false
        description: The image shown below the page title
        type: string
      - name: image_alt
        required: false
        description: The alt text of the image
        type: string
      - name: seo
        required: false
        type: model
        models:
          - SEO
      - name: body
        required: true
        description: Markdown file body
        type: markdown
    match:
      - about.md
      - privacy-policy.md
      - signup.md
      - style-guide.md
      - terms-of-service.md
    type: page
    singleInstance: false
  Post:
    label: Post
    fields:
      - name: title
        required: true
        description: The title of the post
        label: Title
        type: string
      - name: subtitle
        required: false
        description: The text shown just below the title or the featured image
        label: Subtitle
        type: string
      - name: date
        required: true
        label: Date
        type: date
      - name: author
        required: false
        description: Post author
        type: reference
        models:
          - Person
      - name: excerpt
        required: false
        description: The excerpt of the post displayed in the blog feed
        label: Excerpt
        type: string
      - name: image
        required: false
        description: The image shown below the title
        label: Image
        type: string
      - name: image_alt
        required: false
        description: The alt text of the featured image
        label: Image alt text (single post)
        type: string
      - name: thumb_image
        required: false
        description: The image shown in the blog feed
        label: Image (blog feed)
        type: string
      - name: thumb_image_alt
        required: false
        description: The alt text of the blog feed image
        label: Image alt text (blog feed)
        type: string
      - name: seo
        required: false
        type: model
        models:
          - SEO
      - name: body
        required: true
        description: Markdown file body
        type: markdown
    match: blog/**.md
    type: page
    singleInstance: false
  Config:
    label: Site Configuration
    fields:
      - name: title
        required: true
        description: Site title
        label: Title
        type: string
      - name: path_prefix
        required: true
        description: \\"The base URL of this site. Useful for sites hosted under specific
          path, e.g.: https://www.example.com/my-site/\\"
        hidden: true
        label: Base URL
        type: string
      - name: domain
        required: false
        description: The domain of your site, including the protocol, e.g.
          https://mysite.com/
        label: Domain
        type: string
      - name: favicon
        required: false
        description: A square icon that represents your website
        label: Favicon
        type: string
      - name: palette
        required: true
        default: blue
        description: The color palette of the site
        label: Color Palette
        type: enum
        options:
          - blue
          - cyan
          - green
          - orange
          - purple
      - name: base_font
        required: true
        default: nunito-sans
        label: Font
        type: enum
        options:
          - fira-sans
          - nunito-sans
          - system-sans
      - name: header
        required: true
        label: Header Configuration
        type: model
        models:
          - Header
      - name: footer
        required: true
        label: Footer Configuration
        type: model
        models:
          - Footer
      - name: body
        required: true
        description: Markdown file body
        type: markdown
